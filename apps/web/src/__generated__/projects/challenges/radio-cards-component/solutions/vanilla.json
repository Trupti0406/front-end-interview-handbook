    "/css/style.css": {
      "code": "@import url('https://fonts.googleapis.com/css2?family=Noto+Sans:ital,wght@0,100..900;1,100..900&display=swap');\n\n* {\n  box-sizing: border-box;\n  margin: 0;\n}\n\nbody {\n  font-family:\n    'Noto Sans',\n    system-ui,\n    -apple-system,\n    BlinkMacSystemFont,\n    'Segoe UI',\n    Roboto,\n    Oxygen,\n    Ubuntu,\n    Cantarell,\n    'Open Sans',\n    'Helvetica Neue',\n    sans-serif;\n  -webkit-font-smoothing: antialiased;\n  -moz-osx-font-smoothing: grayscale;\n  text-rendering: optimizeLegibility;\n}\n\n.container {\n  display: flex;\n  align-items: center;\n  justify-content: center;\n  flex-direction: column;\n  gap: 24px;\n  margin: 0 auto;\n  min-height: 100vh;\n  padding: 200px 0;\n}\n\n.radio-group {\n  border: none;\n  padding: 0;\n  display: flex;\n  align-items: center;\n  flex-wrap: wrap;\n  justify-content: center;\n  column-gap: 24px;\n  row-gap: 30px;\n}\n.sr-only {\n  position: absolute;\n  width: 1px;\n  height: 1px;\n  padding: 0;\n  margin: -1px;\n  overflow: hidden;\n  clip: rect(0, 0, 0, 0);\n  border: 0;\n}\n\n.radio-button {\n  display: inline-flex;\n  align-items: center;\n  justify-content: center;\n  border-radius: 4px;\n  font-weight: 500;\n  border: none;\n  background: none;\n  outline: inherit;\n  padding: 0;\n  cursor: pointer;\n  font-family: inherit;\n  border: 1px solid #e5e5e5;\n  color: #171717;\n}\n.radio-button:hover {\n  background-color: #fafafa;\n}\n.radio-button:focus {\n  box-shadow: 0px 0px 0px 4px rgba(68, 76, 231, 0.12);\n}\n.radio-group input[type='radio']:disabled + .radio-button {\n  background-color: #f5f5f5;\n  color: #a3a3a3;\n  cursor: not-allowed;\n}\n.radio-group input[type='radio'] {\n  display: none;\n}\n.radio-group input[type='radio']:checked + .radio-button {\n  border: 1px solid #4f46e5;\n}\n\n/* Size classes */\n.radio-button--size-sm {\n  height: 36px;\n  column-gap: 6px;\n  padding: 7px 11px;\n\n  font-size: 14px;\n  line-height: 20px;\n}\n.radio-button--size-md {\n  height: 40px;\n  column-gap: 8px;\n  padding: 9px 13px;\n\n  font-size: 14px;\n  line-height: 20px;\n}\n\n.radio-button--size-lg {\n  height: 44px;\n  column-gap: 8px;\n  padding: 9px 15px;\n\n  font-size: 16px;\n  line-height: 24px;\n}\n\n.radio-button--size-xl {\n  height: 48px;\n  column-gap: 8px;\n  padding: 11px 19px;\n\n  font-size: 16px;\n  line-height: 24px;\n}\n\n.radio-button--size-2xl {\n  height: 60px;\n  column-gap: 12px;\n  padding: 15px 23px;\n\n  font-size: 18px;\n  line-height: 28px;\n}\n"
    },
    "/index.html": {
      "code": "<!doctype html>\n<html lang=\"en\">\n  <head>\n    <meta charset=\"utf-8\" />\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1\" />\n    <title>Radio Cards Component | GreatFrontEnd Projects</title>\n    <link rel=\"stylesheet\" href=\"css/style.css\" />\n  </head>\n\n  <body>\n    <div class=\"container\">\n      <fieldset class=\"radio-group\">\n        <legend class=\"sr-only\">Select an Option</legend>\n        <div>\n          <input type=\"radio\" name=\"option\" value=\"daily\" id=\"daily\" />\n          <label\n            class=\"radio radio-button radio-button--size-sm\"\n            for=\"daily\"\n            tabindex=\"0\">\n            <svg\n              width=\"20\"\n              height=\"20\"\n              viewBox=\"0 0 20 20\"\n              fill=\"none\"\n              xmlns=\"http://www.w3.org/2000/svg\"\n              aria-hidden=\"true\">\n              <path\n                d=\"M3.0556 4.44444C3.0556 4.06092 3.36652 3.75 3.75005 3.75H16.25C16.6336 3.75 16.9445 4.06092 16.9445 4.44444V15.5556C16.9445 15.9391 16.6336 16.25 16.25 16.25H3.75005C3.36652 16.25 3.0556 15.9391 3.0556 15.5556V4.44444ZM4.44449 14.8611H15.5556V7.91667H4.44449V14.8611ZM9.3056 10.6944H5.83338V13.4722H9.3056V10.6944Z\"\n                fill=\"currentColor\" />\n            </svg>\n            <span>Daily</span>\n          </label>\n        </div>\n\n        <div>\n          <input type=\"radio\" name=\"option\" value=\"weekly\" id=\"weekly\" />\n          <label\n            class=\"radio radio-button radio-button--size-md\"\n            for=\"weekly\"\n            tabindex=\"0\"\n            >Weekly</label\n          >\n        </div>\n\n        <div>\n          <input type=\"radio\" name=\"option\" value=\"monthly\" id=\"monthly\" />\n          <label\n            class=\"radio radio-button radio-button--size-lg\"\n            for=\"monthly\"\n            tabindex=\"0\">\n            <span></span>Monthly\n            <svg\n              width=\"20\"\n              height=\"20\"\n              viewBox=\"0 0 20 20\"\n              fill=\"none\"\n              xmlns=\"http://www.w3.org/2000/svg\"\n              aria-hidden=\"true\">\n              <path\n                d=\"M7.91671 2.36111V3.75H12.0834V2.36111H13.4723V3.75H16.25C16.6336 3.75 16.9445 4.06091 16.9445 4.44444V15.5556C16.9445 15.9391 16.6336 16.25 16.25 16.25H3.75005C3.36652 16.25 3.0556 15.9391 3.0556 15.5556V4.44444C3.0556 4.06091 3.36652 3.75 3.75005 3.75H6.52783V2.36111H7.91671ZM15.5556 9.30555H4.44449V14.8611H15.5556V9.30555ZM7.22227 11.3889V12.7778H5.83338V11.3889H7.22227ZM14.1667 11.3889V12.7778H8.61116V11.3889H14.1667ZM6.52783 5.13888H4.44449V7.91666H15.5556V5.13888H13.4723V6.52777H12.0834V5.13888H7.91671V6.52777H6.52783V5.13888Z\"\n                fill=\"currentColor\" />\n            </svg>\n          </label>\n        </div>\n\n        <div>\n          <input\n            type=\"radio\"\n            name=\"option\"\n            value=\"annually\"\n            id=\"annually\"\n            disabled />\n          <label\n            class=\"radio radio-button radio-button--size-xl\"\n            for=\"annually\"\n            tabindex=\"0\">\n            <svg\n              width=\"20\"\n              height=\"20\"\n              viewBox=\"0 0 20 20\"\n              fill=\"none\"\n              xmlns=\"http://www.w3.org/2000/svg\"\n              aria-hidden=\"true\">\n              <path\n                d=\"M7.91665 2.36111V3.75H12.0833V2.36111H13.4722V3.75H16.25C16.6335 3.75 16.9444 4.06091 16.9444 4.44444V15.5556C16.9444 15.9391 16.6335 16.25 16.25 16.25H3.74999C3.36646 16.25 3.05554 15.9391 3.05554 15.5556V4.44444C3.05554 4.06091 3.36646 3.75 3.74999 3.75H6.52776V2.36111H7.91665ZM15.5555 9.30555H4.44443V14.8611H15.5555V9.30555ZM6.52776 5.13888H4.44443V7.91666H15.5555V5.13888H13.4722V6.52777H12.0833V5.13888H7.91665V6.52777H6.52776V5.13888Z\"\n                fill=\"currentColor\" />\n            </svg>\n            <span>Annually</span>\n            <svg\n              width=\"20\"\n              height=\"20\"\n              viewBox=\"0 0 20 20\"\n              fill=\"none\"\n              xmlns=\"http://www.w3.org/2000/svg\"\n              aria-hidden=\"true\">\n              <path\n                d=\"M9.99974 10.8135L13.4371 7.37617L14.4192 8.35826L9.99974 12.7777L5.58032 8.35826L6.56242 7.37617L9.99974 10.8135Z\"\n                fill=\"#A3A3A3\" />\n            </svg>\n          </label>\n        </div>\n\n        <div>\n          <input type=\"radio\" name=\"option\" value=\"calender\" id=\"calender\" />\n          <label\n            class=\"radio radio-button radio-button--size-2xl\"\n            for=\"calender\"\n            tabindex=\"0\">\n            <svg\n              width=\"24\"\n              height=\"24\"\n              viewBox=\"0 0 24 24\"\n              fill=\"none\"\n              xmlns=\"http://www.w3.org/2000/svg\"\n              aria-hidden=\"true\">\n              <path\n                d=\"M3.66663 5.33333C3.66663 4.8731 4.03973 4.5 4.49996 4.5H19.5C19.9602 4.5 20.3333 4.8731 20.3333 5.33333V18.6667C20.3333 19.1269 19.9602 19.5 19.5 19.5H4.49996C4.03973 19.5 3.66663 19.1269 3.66663 18.6667V5.33333ZM5.33329 17.8333H18.6666V9.5H5.33329V17.8333ZM11.1666 12.8333H6.99996V16.1667H11.1666V12.8333Z\"\n                fill=\"currentColor\" />\n            </svg>\n          </label>\n        </div>\n      </fieldset>\n    </div>\n  </body>\n</html>\n"
