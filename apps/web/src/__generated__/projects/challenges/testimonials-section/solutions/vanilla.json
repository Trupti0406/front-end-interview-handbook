    "/css/style.css": {
      "code": "/* Put any custom CSS styles here.  */\n/* Be sure to delete the <style> tag in index.html if you want to use this stylesheet. */\n@import url('https://fonts.googleapis.com/css2?family=Noto+Sans:ital,wght@0,100..900;1,100..900&display=swap');\n\n* {\n  box-sizing: border-box;\n  margin: 0;\n}\n\nbody {\n  font-family:\n    'Noto Sans',\n    system-ui,\n    -apple-system,\n    BlinkMacSystemFont,\n    'Segoe UI',\n    Roboto,\n    Oxygen,\n    Ubuntu,\n    Cantarell,\n    'Open Sans',\n    'Helvetica Neue',\n    sans-serif;\n  min-height: 100vh;\n  background: linear-gradient(147.52deg, #f9fafb 8.89%, #d2d6db 100.48%);\n}\n\n.container {\n  width: 100%;\n  min-height: 100vh;\n  padding: 16px;\n  max-width: 1440px;\n  margin: 0 auto;\n}\n\n.testimonials-section {\n  background: #ffffff;\n  box-shadow: 0px 1px 2px rgba(0, 0, 0, 0.05);\n  border-radius: 4px;\n\n  padding: 48px 12px;\n\n  display: flex;\n  flex-direction: column;\n  gap: 48px;\n}\n\n.header {\n  width: 100%;\n  display: flex;\n  flex-direction: column;\n  gap: 12px;\n}\n\n.header__supporting-title {\n  font-weight: 600;\n  font-size: 20px;\n  line-height: 28px;\n  text-align: center;\n  color: #4338ca;\n}\n\n.header__content {\n  width: 100%;\n  display: flex;\n  flex-direction: column;\n  gap: 20px;\n\n  font-size: 18px;\n  line-height: 28px;\n  text-align: center;\n  color: #525252;\n}\n\n.header__content p {\n  padding: 0 30px;\n}\n\n.header__content__title {\n  font-weight: 600;\n  font-size: 30px;\n  line-height: 36px;\n  text-align: center;\n  color: #171717;\n}\n\n.testimonials-list {\n  column-count: 1;\n  column-gap: 24px;\n}\n\n.testimonial-card {\n  background: #ffffff;\n  padding: 23px;\n  display: flex;\n  flex-direction: column;\n  gap: 16px;\n  border: 1px solid #e6e6e6;\n  box-shadow:\n    0px 1px 3px rgba(0, 0, 0, 0.1),\n    0px 1px 2px rgba(0, 0, 0, 0.06);\n  border-radius: 8px;\n  color: #525252;\n  font-size: 16px;\n  line-height: 24px;\n  break-inside: avoid;\n}\n\n.testimonial-card:not(:first-child) {\n  margin-top: 24px;\n}\n\n.testimonial-card__author {\n  display: flex;\n  gap: 16px;\n  align-items: center;\n}\n\n.testimonial-card__author img {\n  width: 48px;\n  height: 48px;\n  border-radius: 50%;\n}\n\n.testimonial-card__author__info {\n  display: flex;\n  flex-direction: column;\n  gap: 1px;\n}\n\n.testimonial-card__author__info__name {\n  font-weight: 600;\n  font-size: 18px;\n  line-height: 28px;\n  color: #171717;\n}\n\n.testimonial-card__author__info__username {\n  font-size: 14px;\n  line-height: 20px;\n}\n\n@media screen and (min-width: 768px) {\n  .testimonials-section {\n    box-shadow:\n      0px 4px 6px -1px rgba(0, 0, 0, 0.1),\n      0px 2px 4px -1px rgba(0, 0, 0, 0.06);\n    padding: 64px 16px;\n    gap: 64px;\n  }\n\n  .header__content {\n    font-size: 20px;\n  }\n\n  .header__content__title {\n    font-size: 48px;\n    line-height: 48px;\n  }\n\n  .testimonials-list {\n    column-count: 2;\n    column-gap: 32px;\n  }\n\n  .testimonial-card:not(:first-child) {\n    margin-top: 38px;\n  }\n}\n\n@media screen and (min-width: 1024px) {\n  .testimonials-section {\n    box-shadow:\n      0px 10px 15px -3px rgba(0, 0, 0, 0.1),\n      0px 4px 6px -2px rgba(0, 0, 0, 0.05);\n    border-radius: 6px;\n\n    padding: 96px;\n  }\n\n  .header__content__title {\n    font-size: 48px;\n    line-height: 48px;\n  }\n\n  .testimonials-list {\n    column-count: 3;\n    column-gap: 32px;\n  }\n\n  .testimonial-card:not(:first-child) {\n    margin-top: 32px;\n  }\n}\n"
    },
    "/index.html": {
      "code": "<!doctype html>\n<html lang=\"en\">\n  <head>\n    <meta charset=\"utf-8\" />\n    <meta name=\"viewport\" content=\"width=device-width, initial-scale=1\" />\n    <title>Testimonials Section | GreatFrontEnd Projects</title>\n    <link rel=\"stylesheet\" href=\"css/style.css\" />\n  </head>\n  <body>\n    <main class=\"container\">\n      <section\n        class=\"testimonials-section\"\n        aria-labelledby=\"testimonial-section\">\n        <header class=\"header\">\n          <div class=\"header__supporting-title\">Testimonials</div>\n          <div class=\"header__content\">\n            <h2 class=\"header__content__title\">\n              Countless users, countless smiles\n            </h2>\n            <p>\n              Explore our community's journey and discover why satisfaction\n              defines us.\n            </p>\n          </div>\n        </header>\n\n        <div class=\"testimonials-list\">\n          <figure class=\"testimonial-card\">\n            <figcaption class=\"testimonial-card__author\">\n              <img\n                src=\"https://vaqybtnqyonvlwtskzmv.supabase.co/storage/v1/object/public/projects-images/testimonials-section/starter/img/sarah.jpg\"\n                alt=\"Sarah Dole's profile picture\" />\n              <div class=\"testimonial-card__author__info\">\n                <div class=\"testimonial-card__author__info__name\">\n                  Sarah Dole\n                </div>\n                <div class=\"testimonial-card__author__info__username\">\n                  @sarahdole\n                </div>\n              </div>\n            </figcaption>\n            <blockquote>\n              <p>\n                I've been searching for high-quality abstract images for my\n                design projects, and I'm thrilled to have found this platform.\n                The variety and depth of creativity are astounding!\n              </p>\n            </blockquote>\n          </figure>\n          <figure class=\"testimonial-card\">\n            <figcaption class=\"testimonial-card__author\">\n              <img\n                src=\"https://vaqybtnqyonvlwtskzmv.supabase.co/storage/v1/object/public/projects-images/testimonials-section/starter/img/john.jpg\"\n                alt=\"Marcus Thompson's profile picture\" />\n              <div class=\"testimonial-card__author__info\">\n                <div class=\"testimonial-card__author__info__name\">\n                  John Appleseed\n                </div>\n                <div class=\"testimonial-card__author__info__username\">\n                  @johnaseed\n                </div>\n              </div>\n            </figcaption>\n            <blockquote>\n              <p>\n                As an artist, finding inspiration is crucial. This platform has\n                become my go-to source for unique abstract images that ignite my\n                creativity like never before.\n              </p>\n            </blockquote>\n          </figure>\n          <figure class=\"testimonial-card\">\n            <figcaption class=\"testimonial-card__author\">\n              <img\n                src=\"https://vaqybtnqyonvlwtskzmv.supabase.co/storage/v1/object/public/projects-images/testimonials-section/starter/img/jean.jpg\"\n                alt=\"Jean Gray's profile picture\" />\n              <div class=\"testimonial-card__author__info\">\n                <div class=\"testimonial-card__author__info__name\">\n                  Jean Gray\n                </div>\n                <div class=\"testimonial-card__author__info__username\">\n                  @jeniic\n                </div>\n              </div>\n            </figcaption>\n            <blockquote>\n              <p>\n                I never thought I'd find such stunning abstract images for free!\n                This platform has exceeded my expectations in every way.\n              </p>\n            </blockquote>\n          </figure>\n          <figure class=\"testimonial-card\">\n            <figcaption class=\"testimonial-card__author\">\n              <img\n                src=\"https://vaqybtnqyonvlwtskzmv.supabase.co/storage/v1/object/public/projects-images/testimonials-section/starter/img/jake.jpg\"\n                alt=\"Jake Johnson's profile picture\" />\n              <div class=\"testimonial-card__author__info\">\n                <div class=\"testimonial-card__author__info__name\">\n                  Jake Johnson\n                </div>\n                <div class=\"testimonial-card__author__info__username\">\n                  @jakejohnshon\n                </div>\n              </div>\n            </figcaption>\n            <blockquote>\n              <p>\n                From corporate presentations to personal projects, the abstract\n                images on this platform have added a touch of elegance and\n                sophistication to everything I create.\n              </p>\n            </blockquote>\n          </figure>\n          <figure class=\"testimonial-card\">\n            <figcaption class=\"testimonial-card__author\">\n              <img\n                src=\"https://vaqybtnqyonvlwtskzmv.supabase.co/storage/v1/object/public/projects-images/testimonials-section/starter/img/igor.jpg\"\n                alt=\"Igor Stravinsky's profile picture\" />\n              <div class=\"testimonial-card__author__info\">\n                <div class=\"testimonial-card__author__info__name\">\n                  Igor Stravinsky\n                </div>\n                <div class=\"testimonial-card__author__info__username\">\n                  @igorstrav\n                </div>\n              </div>\n            </figcaption>\n            <blockquote>\n              <p>\n                The subscription plans are worth every penny. Having unlimited\n                access to premium abstract images has transformed my design\n                workflow and elevated the quality of my work\n              </p>\n            </blockquote>\n          </figure>\n          <figure class=\"testimonial-card\">\n            <figcaption class=\"testimonial-card__author\">\n              <img\n                src=\"https://vaqybtnqyonvlwtskzmv.supabase.co/storage/v1/object/public/projects-images/testimonials-section/starter/img/declan.jpg\"\n                alt=\"Declan Rice's profile picture\" />\n              <div class=\"testimonial-card__author__info\">\n                <div class=\"testimonial-card__author__info__name\">\n                  Declan Rice\n                </div>\n                <div class=\"testimonial-card__author__info__username\">\n                  @drice\n                </div>\n              </div>\n            </figcaption>\n            <blockquote>\n              <p>\n                I'm amazed by the attention to detail in every image on this\n                platform. It's clear that a lot of thought and creativity goes\n                into curating the collection.\n              </p>\n            </blockquote>\n          </figure>\n          <figure class=\"testimonial-card\">\n            <figcaption class=\"testimonial-card__author\">\n              <img\n                src=\"https://vaqybtnqyonvlwtskzmv.supabase.co/storage/v1/object/public/projects-images/testimonials-section/starter/img/marcus.jpg\"\n                alt=\"Marcus Thompson's profile picture\" />\n              <div class=\"testimonial-card__author__info\">\n                <div class=\"testimonial-card__author__info__name\">\n                  Marcus Thompson\n                </div>\n                <div class=\"testimonial-card__author__info__username\">\n                  @mthompson\n                </div>\n              </div>\n            </figcaption>\n            <blockquote>\n              <p>\n                Using abstract images from this platform has helped me convey\n                complex concepts in a visually engaging way. My clients are\n                always impressed!\n              </p>\n            </blockquote>\n          </figure>\n          <figure class=\"testimonial-card\">\n            <figcaption class=\"testimonial-card__author\">\n              <img\n                src=\"https://vaqybtnqyonvlwtskzmv.supabase.co/storage/v1/object/public/projects-images/testimonials-section/starter/img/oliver.jpg\"\n                alt=\"Oliver Neverloved's profile picture\" />\n              <div class=\"testimonial-card__author__info\">\n                <div class=\"testimonial-card__author__info__name\">\n                  Oliver Neverloved\n                </div>\n                <div class=\"testimonial-card__author__info__username\">\n                  @olivernever\n                </div>\n              </div>\n            </figcaption>\n            <blockquote>\n              <p>\n                I appreciate how user-friendly the platform is. Browsing,\n                downloading, and managing my image library couldn't be easier.\n              </p>\n            </blockquote>\n          </figure>\n          <figure class=\"testimonial-card\">\n            <figcaption class=\"testimonial-card__author\">\n              <img\n                src=\"https://vaqybtnqyonvlwtskzmv.supabase.co/storage/v1/object/public/projects-images/testimonials-section/starter/img/mark.jpg\"\n                alt=\"Mark Dennis's profile picture\" />\n              <div class=\"testimonial-card__author__info\">\n                <div class=\"testimonial-card__author__info__name\">\n                  Mark Dennis\n                </div>\n                <div class=\"testimonial-card__author__info__username\">\n                  @marked\n                </div>\n              </div>\n            </figcaption>\n            <blockquote>\n              <p>\n                The customer support team went above and beyond to help me with\n                a subscription issue. Their dedication to customer satisfaction\n                is truly commendable\n              </p>\n            </blockquote>\n          </figure>\n        </div>\n      </section>\n    </main>\n  </body>\n</html>\n"
    },
