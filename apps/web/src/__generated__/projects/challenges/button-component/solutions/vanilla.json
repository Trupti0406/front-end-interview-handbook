    "/css/style.css": {
      "code": "@import url('https://fonts.googleapis.com/css2?family=Noto+Sans:ital,wght@0,100..900;1,100..900&display=swap');\n\n* {\n  box-sizing: border-box;\n  margin: 0;\n}\n\nbody {\n  font-family:\n    'Noto Sans',\n    system-ui,\n    -apple-system,\n    BlinkMacSystemFont,\n    'Segoe UI',\n    Roboto,\n    Oxygen,\n    Ubuntu,\n    Cantarell,\n    'Open Sans',\n    'Helvetica Neue',\n    sans-serif;\n  -webkit-font-smoothing: antialiased;\n  -moz-osx-font-smoothing: grayscale;\n  text-rendering: optimizeLegibility;\n}\n\n.container {\n  display: flex;\n  justify-content: center;\n  align-items: flex-start;\n  margin: 0 auto;\n  min-height: 100vh;\n  padding: 112px 24px;\n}\n\n.button__variants {\n  display: flex;\n  flex-direction: column;\n  gap: 48px;\n  width: 300px;\n}\n\n.button__variants__sizes {\n  display: flex;\n  align-items: center;\n  flex-wrap: wrap;\n  gap: 20px;\n}\n\n.button {\n  display: inline-flex;\n  align-items: center;\n  justify-content: center;\n  border-radius: 4px;\n  font-weight: 500;\n  border: none;\n  background: none;\n  outline: inherit;\n  padding: 0;\n  cursor: pointer;\n  font-family: inherit;\n}\n.button:focus {\n  box-shadow: 0px 0px 0px 4px rgba(68, 76, 231, 0.12);\n}\n\n/* Size classes */\n.button--size-md {\n  height: 40px;\n  column-gap: 6px;\n  padding: 10px 14px;\n\n  font-size: 14px;\n  line-height: 20px;\n}\n.button--secondary.button--size-md {\n  padding: 9px 13px;\n}\n\n.button--size-lg {\n  height: 44px;\n  column-gap: 8px;\n  padding: 10px 16px; \n\n  font-size: 16px;\n  line-height: 24px;\n}\n.button--secondary.button--size-lg {\n  padding: 9px 15px; \n}\n\n.button--size-xl {\n  height: 48px;\n  column-gap: 8px;\n  padding: 12px 20px;\n\n  font-size: 16px;\n  line-height: 24px;\n}\n.button--secondary.button--size-xl {\n  padding: 11px 19px; \n}\n\n.button--size-2xl {\n  height: 60px;\n  column-gap: 12px;\n  padding: 16px 24px; \n\n  font-size: 18px;\n  line-height: 28px;\n}\n.button--secondary.button--size-2xl {\n  padding: 15px 23px; \n}\n\n.button--link.button--size-md,\n.button--link.button--size-lg,\n.button--link.button--size-xl,\n.button--link.button--size-2xl  {\n  padding: 0;\n  height: 100%;\n}\n\n.button--icon-size-md {\n  width: 40px;\n}\n.button--icon-size-lg {\n  width: 44px;\n}\n.button--icon-size-xl {\n  width: 48px;\n}\n.button--icon-size-2xl {\n  width: 56px;\n  height: 56px;\n}\n\n/* button variants */\n.button--primary{\n  background: #4338CA;\n  box-shadow: 0px 1px 3px rgba(0, 0, 0, 0.1), 0px 1px 2px rgba(0, 0, 0, 0.06);\n  color: #ffffff;\n}\n.button--primary:hover, .button--primary:focus  {\n  background: #3730A3;\n}\n.button--primary:disabled, .button--secondary:disabled {\n  background: #F5F5F5;\n  box-shadow: none;\n  color: #A3A3A3;\n  pointer-events: none;\n}\n\n.button--secondary{\n  border: 0.5px solid #E6E6E6;\n  background: #FFFFFF;\n  box-shadow: 0px 1px 3px rgba(0, 0, 0, 0.1), 0px 1px 2px rgba(0, 0, 0, 0.06);\n  color: #171717;\n}\n.button--secondary:hover, .button--secondary:focus {\n  background: #FAFAFA;\n  border: 1px solid #E6E6E6;\n  color: #0A0A0A\n}\n.button--secondary:focus {\n  box-shadow: 0px 0px 0px 4px rgba(68, 76, 231, 0.12);\n}\n\n.button_tertiary{\n  color: #4338CA;\n}\n.button_tertiary:hover, .button_tertiary:focus {\n  background: #FAFAFA;\n}\n.button_tertiary:disabled, .button--danger:disabled  {\n  background: none;\n  box-shadow: none;\n  color: #A3A3A3;\n  pointer-events: none;\n}\n\n.button--danger{\n  background: #DC2626;\n  color: #ffffff;\n}\n.button--danger:hover, .button--danger:focus  {\n  background: #B91C1C;;\n}\n.button--danger:focus {\n  box-shadow: 0px 0px 0px 1px #D92D20, 0px 0px 0px 4px rgba(217, 45, 32, 0.12);\n}\n\n.button--link {\n  color: #4338CA;\n}\n.button--link:hover, .button--link:focus {\n  color: #3730A3;\n}\n\n.button--link-gray {\n  color: #525252;\n}\n.button--link-gray:hover, .button--link-gray:focus {\n  color: #171717;\n}\n\n@media screen and (min-width: 768px) {\n  .container {\n    padding-left: 32px;\n    padding-right: 32px;\n    padding-bottom: 0;\n  }\n  .button__variants {\n    width: auto;\n  }\n}"
