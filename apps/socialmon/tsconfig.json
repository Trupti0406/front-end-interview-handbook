{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "@gfe/tsconfig/nextjs.json",
  "compilerOptions": {
    "plugins": [
      {
        "name": "next"
      }
    ],
    "baseUrl": "./src",
    "paths": {
      "~/*": ["*"],
      "~/prisma/client": ["../prisma/generated/client"]
    },
    "strictNullChecks": true
  },
  "include": ["next-env.d.ts", "src", ".next/types/**/*.ts", "types.d.ts"]
}
