{
  "name": "gfe",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --filter=greatfrontend",
    "dev:all": "turbo run dev",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,md,mdx}\"",
    "tsc": "turbo run tsc"
  },
  "devDependencies": {
    "prettier": "latest",
    "turbo": "latest"
  },
  "engines": {
    "npm": ">=8.5.0",
    "node": ">=16.0.0"
  },
  "dependencies": {
    "react-circular-progressbar": "^2.1.0"
  }
}
